.list{
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 40rpx;
  box-sizing: border-box;
}

.list image{
  width: 20rpx;
  height: 20rpx;
}

.list .left{
  display: flex;
  align-items: center;
}


.list .left image{
  margin-right: 20rpx;
  width: 40rpx;
  height: 40rpx;
}